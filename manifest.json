{
  "manifest_version": 2,
  "name": "ContainerScript",
  "version": "1.0",
  "description": "User defined scripts for assining URLs to containers",
  "permissions": [
    "<all_urls>",
    "contextualIdentities",
    "cookies",
    "webRequestBlocking",
    "webRequest",
    "tabs"
  ],
  "background": {
    "scripts": ["./dist/background.bundle.js"]
  },
  "browser_action": {
    "default_title": "ContainerScript"
  },
  "web_accessible_resources": ["ui.html"]
}
